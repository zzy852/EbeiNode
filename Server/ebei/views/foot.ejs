<script src="/javascripts/jquery-1.11.1.min.js"></script>
<script src="/bootstrap-3.2.0/js/bootstrap.min.js"></script>
<script src="/jquery.fine-uploader-5.1.3/jquery.fine-uploader.js"></script>
<script src="/javascripts/is.min.js"></script>
<script>
	$(document).ready(function() {
		if ($("input.empty").length > 0) {
			$("input.empty").each(function(index, el) {
				var me = $(el);
				me.focus();
				if (me.val() != "") {
					me.removeClass('empty');
				}
				if (index == $(".empty").length - 1) {
					$($(".empty")[0]).focus();
				}
			});
		}

		$('#ebei-navbar-collapse').on('show.bs.collapse', function() {
			$("#ebei-navbar-toggle").addClass('ebei-navbar-close')
		});
		$('#ebei-navbar-collapse').on('hide.bs.collapse', function() {
			$("#ebei-navbar-toggle").removeClass('ebei-navbar-close')
		});
	});
</script>